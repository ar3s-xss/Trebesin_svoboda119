\section{Databázová struktura}
\subsection{ER diagram}
\includegraphics[width=160mm]{ER Diagram.png} 
\subsection{Tabulka Person}
\subsubsection{Struktura}
	first_name varchar(15) \\
  last_name varchar(30) \\
  middle_name varchar(15) \\
  sex char(1) \\
  birth_number varchar(11) \\
  family_status varchar(15) \\
  email_address varchar(25) \\
  phone_number varchar(15) \\
  facebook_id varchar(50) \\
  `instagram_id` varchar(50) \\
  `id_ci_sex` bigint \\
  `id_ci_marital_status` bigint \\
  `id_ci_nationality` bigint \\
\subsubsection{Provázání tabulky}
tabulka struktura je provazana one to many
